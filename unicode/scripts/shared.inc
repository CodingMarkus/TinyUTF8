#!/bin/sh
set -e -u

checkForUnicodeDataFile( )
{
	if [ ! -f "UnicodeData.txt" ]
	then
		echo "File UnicodeData.txt not found!" >&2
		exit 1
	fi
	printf 'UnicodeData.txt'
}


printHeaderComment( )
{
	scriptName=$( basename "$0" )
	output="
/*
	This file has been auto-generated by the script
	$scriptName

	If the file content is incorrect, do not edit the file directly,
	instead edit the script and have the file regenerated by it.
*/
"
	printf '%s' "$output"
}
